{
  "title": "Web Book",
  "author": "Chris Song",
  "description": "web 知识汇总",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "styles": {
    "website": "./styles/website.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "plugins": [
    "back-to-top-button",
    "chapter-fold",
    "code",
    "splitter",
    "-lunr",
    "-search",
    "search-pro",
    "popup",
    "-sharing",
    "sharing-plus"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/song111/webBook"
    },

    "sharing": {
      "douban": true,
      "facebook": true,
      "google": true,
      "pocket": true,
      "qq": true,
      "qzone": true,
      "twitter": true,
      "weibo": true,
      "all": [
        "douban",
        "facebook",
        "google",
        "instapaper",
        "linkedin",
        "twitter",
        "weibo",
        "messenger",
        "qq",
        "qzone",
        "viber",
        "whatsapp"
      ]
    }
  }
}
